<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.image.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<StackPane xmlns="http://javafx.com/javafx/17.0.2-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.pizza.sundevilpizza.StartUpController">
   <children>
      <ImageView fitHeight="597.0" fitWidth="897.0">
         <image>
            <Image url="@image/Background_Pic.jpeg" />
         </image>
      </ImageView>
      <AnchorPane prefHeight="400.0" prefWidth="600.0">
         <children>
            <ImageView fitHeight="90.0" fitWidth="97.0" layoutX="404.0" layoutY="65.0" preserveRatio="true">
               <image>
                  <Image url="@image/pizza-logo.png" />
               </image>
            </ImageView>
            <Label layoutX="318.0" layoutY="182.0" text="SunDevil Pizza" textFill="WHITE">
               <font>
                  <Font name="Comic Sans MS Bold" size="41.0" />
               </font>
            </Label>
            <Button layoutX="246.0" layoutY="341.0" mnemonicParsing="false" onAction="#switchToCustomerPage" text="Student" textFill="#070505">
               <font>
                  <Font name="Comic Sans MS" size="21.0" />
               </font>
               <opaqueInsets>
                  <Insets />
               </opaqueInsets>
            </Button>
            <Button layoutX="408.0" layoutY="340.0" mnemonicParsing="false" onAction="#toChefPage" text="Chef" textFill="#070303">
               <font>
                  <Font name="Comic Sans MS" size="21.0" />
               </font>
            </Button>
            <Button layoutX="518.0" layoutY="340.0" mnemonicParsing="false" onAction="#toProcessingAgent" text="Processing Agent" textFill="#0d0808">
               <font>
                  <Font name="Comic Sans MS" size="21.0" />
               </font>
            </Button>
         </children></AnchorPane>
   </children>
</StackPane>
