<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.StackPane?>
<?import javafx.scene.text.Font?>

<StackPane xmlns="http://javafx.com/javafx/17.0.2-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.pizza.sundevilpizza.OrderStatusController">
   <children>
      <ImageView fitHeight="597.0" fitWidth="897.0">
         <image>
            <Image url="@image/Background_Pic.jpeg" />
         </image>
      </ImageView>
      <AnchorPane prefHeight="400.0" prefWidth="600.0">
         <children>
            <ImageView fitHeight="90.0" fitWidth="97.0" layoutX="403.0" layoutY="34.0" preserveRatio="true">
               <image>
                  <Image url="@image/pizza-logo.png" />
               </image>
            </ImageView>
            <Label layoutX="306.0" layoutY="160.0" text="SunDevil Pizza" textFill="WHITE">
               <font>
                  <Font name="Comic Sans MS Bold" size="41.0" />
               </font>
            </Label>
            <Label layoutX="360.0" layoutY="218.0" text="Order Status" textFill="WHITE">
               <font>
                  <Font size="32.0" />
               </font>
            </Label>
            <Label fx:id="statusLabel" alignment="CENTER" layoutX="154.0" layoutY="299.0" prefHeight="55.0" prefWidth="588.0" text="Ready to Cook!" textAlignment="CENTER" textFill="#ff81ab">
               <font>
                  <Font size="38.0" />
               </font>
            </Label>
            <Button layoutX="56.0" layoutY="38.0" mnemonicParsing="false" onAction="#switchToStartUpPage" prefHeight="25.0" prefWidth="49.0" style="-fx-background-color: #ffffff;" text="Back" textFill="#535252">
               <font>
                  <Font name="Comic Sans MS Bold" size="12.0" />
               </font></Button>
            <Label fx:id="statusLabel2" alignment="CENTER" layoutX="155.0" layoutY="354.0" prefHeight="55.0" prefWidth="588.0" textAlignment="CENTER" textFill="#ff81ab">
               <font>
                  <Font size="38.0" />
               </font>
            </Label>
         </children></AnchorPane>
   </children>
</StackPane>
